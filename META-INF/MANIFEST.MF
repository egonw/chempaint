Manifest-Version: 1.0
Bundle-ManifestVersion: 2
Bundle-Name: ChemView
Bundle-SymbolicName: pvplugins-chempaint
Bundle-Version: 2.0.1
Bundle-Description: PathVisio plugin that shows 2D diagrams for
  metabolites if the SMILES is given in the BridgeDb metabolite
  identifier database.
Bundle-License: http://www.apache.org/licenses/LICENSE-2.0.txt
Bundle-DocURL: https://github.com/egonw/chempaint
Bundle-Icon: chempaintPathVisioIcon.png
Bundle-ClassPath: lib/beam-core-0.9.1.jar,
