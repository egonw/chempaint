  lib/cdk-core-1.5.14-SNAPSHOT.jar,
  lib/cdk-depict-1.5.14-SNAPSHOT.jar,
  lib/cdk-interfaces-1.5.14-SNAPSHOT.jar,
  lib/cdk-isomorphism-1.5.14-SNAPSHOT.jar,
  lib/cdk-render-1.5.14-SNAPSHOT.jar,
  lib/cdk-renderawt-1.5.14-SNAPSHOT.jar,
  lib/cdk-renderbasic-1.5.14-SNAPSHOT.jar,
  lib/cdk-sdg-1.5.14-SNAPSHOT.jar,
  lib/cdk-silent-1.5.14-SNAPSHOT.jar,
  lib/cdk-smiles-1.5.14-SNAPSHOT.jar,
  lib/cdk-standard-1.5.14-SNAPSHOT.jar,
  lib/guava-17.0.jar,
  lib/jgrapht.jar,
  lib/vecmath1.2.jar,
  pvplugins-bridgedbSettings
Export-Package: org.pathvisio.plugins.chempaint
Require-Bundle: org.pathvisio.core;bundle-version="2.0.11",
 org.pathvisio.gui;bundle-version="2.0.11",
 org.pathvisio.desktop;bundle-version="2.0.11",
 org.bridgedb;bundle-version="1.1.0",
 org.bridgedb.bio;bundle-version="1.1.0"
Import-Package: org.osgi.framework;version="1.5.0"
Bundle-Activator: org.pathvisio.plugins.chempaint.Activator
